  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nrjdalal/create-next-pro.git"
  },
  "scripts": {
    "build": "npx tsc",
    "test": "bun run build && rm -rf test && mkdir test && cd test && bun ../dist/index.js",
    "minor": "bun run test && git add . && git commit -m 'minor' && np minor --no-release-draft --yolo=true",
    "major": "bun run test && git add . && git commit -m 'major' && np major --yolo=true"
  },
  "type": "module",
  "bin": {
    "create-next-pro": "dist/index.js"
  },
  "dependencies": {
    "@inquirer/confirm": "^3.0.0",
    "@inquirer/select": "^2.0.0",
    "chalk": "^5.3.0",
    "execa": "^8.0.1",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^20.11.19",
    "np": "^9.2.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  }
}
